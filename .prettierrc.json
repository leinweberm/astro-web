{
    "tabWidth": 4,
    "useTabs": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "jsxSingleQuote": false,
    "printWidth": 110,
    "singleAttributePerLine": true,
    "bracketSameLine": false,
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "plugins": [
        "prettier-plugin-astro"
    ],
    "overrides": [
        {
            "files": "*.astro",
            "options": {
                "parser": "astro"
            }
        }
    ]
}